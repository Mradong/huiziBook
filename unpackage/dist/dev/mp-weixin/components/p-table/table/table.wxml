<view class="content"><block wx:for="{{tableData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="custom-area"><uni-swipe-action vue-id="{{'1-'+index}}" bind:__l="__l" vue-slots="{{['default']}}"><uni-swipe-action-item vue-id="{{('2-'+index)+','+('1-'+index)}}" options="{{options}}" data-event-opts="{{[['^click',[['onClick',['$0'],[[['tableData','',index,'id']]]]]],['^change',[['change']]]]}}" bind:click="__e" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="custom-area-item"><view class="tc thead"><view class="tc-r">{{"编码："+item.id}}</view><view class="tc-l">{{item.isfull?'已取':'未取'}}</view></view><view class="td"><view class="td-r"><view>{{"每月缴费时间："+(item.subitems_timemodel=='新历'?item.subitems_new_onetime+'号':item.subitems_old_show_onetime)}}</view><view>{{"开始时间："+item.start_time}}</view><view>{{"结束时间："+item.end_time}}</view></view><view class="td-l"><view>{{"已缴费期数："+item.payment_num+"期"}}</view><view>{{"已缴费金额："+item.delivered}}</view><view>{{"已收益金额："+item.earned_surplus}}</view></view></view><view class="tf"><view class="tf-r">{{"项目名："+item.subitems_name}}</view><view class="tf-l"><button class="tf-button" type="default" data-event-opts="{{[['tap',[['toLists',['$0'],[[['tableData','',index,'id']]]]]]]}}" bindtap="__e">查看详情</button></view></view></view></uni-swipe-action-item></uni-swipe-action></view></block></view>