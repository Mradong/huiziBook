<view><nav-bar vue-id="1" fontColor="#000" backState="1000" home="{{true}}" titleCenter="{{true}}" type="fixed" title="录入/修正 缴费" bind:__l="__l"></nav-bar><view class="form-one"><block wx:if="{{!isfull}}"><view class="get-full ">当期若为<text>取会期,请</text><button data-event-opts="{{[['tap',[['isFetch',['$event']]]]]}}" bindtap="__e">点我</button></view></block><block wx:if="{{isfull&&iscurrent}}"><view class="get-full">当期为<text>取会期</text></view></block><block wx:if="{{isfull&&!iscurrent}}"><view class="get-full">已有<text>取会期</text></view></block><view class="form-row"><label class="form-row-title">金额</label><input class="form-row-input " type="text" maxlength="11" placeholder="请输入正确缴费" data-event-opts="{{[['input',[['__set_model',['','money','$event',[]]]]]]}}" value="{{money}}" bindinput="__e"/><block wx:if="{{money==''?true:false}}"><view class="warn"></view></block></view></view><button class="create" type="warn" data-event-opts="{{[['tap',[['changeMoney',['$0','$1'],['id','num']]]]]}}" bindtap="__e">完成</button><uni-popup class="vue-ref" vue-id="2" type="dialog" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}"><uni-popup-dialog vue-id="{{('3')+','+('2')}}" type="error" title="警告" mode="base" content="请再次确认,当期是否为取会期" duration="{{2000}}" before-close="{{true}}" data-event-opts="{{[['^close',[['close']]],['^confirm',[['confirm']]]]}}" bind:close="__e" bind:confirm="__e" bind:__l="__l"></uni-popup-dialog></uni-popup></view>