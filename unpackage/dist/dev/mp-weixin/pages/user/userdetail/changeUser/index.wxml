<view><nav-bar vue-id="1" fontColor="#000" backState="1000" home="{{true}}" titleCenter="{{true}}" type="fixed" title="信息修改" bind:__l="__l"></nav-bar><view class="form-one"><view class="form-row"><label class="form-row-title">姓名：</label><input class="form-row-input " type="text" maxlength="11" placeholder="请输入项目人姓名" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/><block wx:if="{{name==''?true:false}}"><view class="warn"><image src="../../../../static/images/cuowu.svg" mode></image></view></block></view><view class="form-row"><label class="form-row-title">电话：</label><input class="form-row-input " type="text" maxlength="11" placeholder="请输入项目人电话" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/><block wx:if="{{phone==''?true:false}}"><view class="warn"><image src="../../../../static/images/cuowu.svg" mode></image></view></block></view><view class="form-row"><label class="form-row-title">描述：</label><input class="form-row-input " type="text" maxlength="11" placeholder="请输入项目人描述" data-event-opts="{{[['input',[['__set_model',['','description','$event',[]]]]]]}}" value="{{description}}" bindinput="__e"/></view><view class="form-row"><label class="form-row-title">地址：</label><input class="form-row-input " type="text" maxlength="11" placeholder="请输入项目人地址" data-event-opts="{{[['input',[['__set_model',['','site','$event',[]]]]]]}}" value="{{site}}" bindinput="__e"/></view></view><button class="create" type="warn" data-event-opts="{{[['tap',[['changeData']]]]}}" bindtap="__e">完成</button></view>