.data-nav {
  height: 120rpx;
  line-height: 120rpx;
  border-bottom: 1px #C0C0C0;
  box-shadow: 0 5px 5px #F1F1F1;
  text-align: center;
  position: relative;
}
.data-nav-add {
  margin-right: 10px;
  width: 60rpx;
  text-align: center;
  font-size: 28px;
  background-color: #FFFFFF;
}
.data-user-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 140rpx;
  border-bottom: 1px solid #C0C0C0;
}
.data-user-item-logo {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  position: relative;
}
.data-user-item-logo-ball {
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  line-height: 40rpx;
  background-color: red;
  position: absolute;
  right: 15rpx;
  top: 15rpx;
  border-radius: 50%;
  z-index: 9;
  color: #fff;
}
.data-user-item-logo image {
  margin-top: 20rpx;
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
}
.data-user-item-detail {
  -webkit-box-flex: 4;
  -webkit-flex: 4;
          flex: 4;
  padding-left: 50rpx;
  line-height: 70rpx;
}
.data-user-item-detail view {
  height: 50%;
  color: #383838;
  font-size: 14px;
}
.icon_setUp image {
  width: 35rpx;
  height: 35rpx;
  position: relative;
  top: -10rpx;
}

