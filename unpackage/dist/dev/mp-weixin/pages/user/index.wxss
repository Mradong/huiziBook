.data-nav {
  height: 120rpx;
  line-height: 120rpx;
  border-bottom: 1px #C0C0C0;
  box-shadow: 0 5px 5px #F1F1F1;
  text-align: center;
  position: relative;
}
.data-nav-add {
  margin-right: 10px;
  width: 60rpx;
  text-align: center;
  font-size: 28px;
  background-color: #FFFFFF;
}
.data-user-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 140rpx;
  border-bottom: 1px solid #C0C0C0;
}
.data-user-item-logo {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  position: relative;
}
.data-user-item-logo-ball {
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  line-height: 40rpx;
  background-color: red;
  position: absolute;
  right: 15rpx;
  top: 15rpx;
  border-radius: 50%;
  z-index: 9;
  color: #fff;
}
.data-user-item-logo image {
  margin-top: 20rpx;
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
}
.data-user-item-detail {
  -webkit-box-flex: 4;
  -webkit-flex: 4;
          flex: 4;
  padding-left: 50rpx;
  line-height: 70rpx;
}
.data-user-item-detail view {
  height: 50%;
  color: #383838;
  font-size: 14px;
}
.icon_setUp image {
  width: 35rpx;
  height: 35rpx;
  position: relative;
  top: -10rpx;
}
.none-userData {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  text-align: center;
}
.none-userData-img {
  width: 320rpx;
  height: 320rpx;
  position: absolute;
  top: 50%;
  right: 50%;
  margin-top: -160rpx;
  margin-right: -160rpx;
}
.none-userData-img image {
  width: 120rpx;
  height: 120rpx;
}
.none-userData-img-text {
  color: #a6a6a6;
  font-size: 13px;
}
.none-userData-img-button {
  width: 160rpx;
  height: 60rpx;
  margin: 20rpx auto;
}
.none-userData-img-button button {
  width: 100%;
  height: 100%;
  font-size: 24rpx;
  color: #a6a6a6;
}

