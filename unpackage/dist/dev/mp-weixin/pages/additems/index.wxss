.form-row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  border-bottom: 1px solid #C8C7CC;
  margin: 10rpx 20rpx;
}
.form-row-title {
  -webkit-box-flex: 3;
  -webkit-flex: 3;
          flex: 3;
}
.form-row-input {
  -webkit-box-flex: 5;
  -webkit-flex: 5;
          flex: 5;
  height: 100rpx;
  text-align: left;
}
.but {
  margin-top: 200rpx;
  width: 80%;
}

